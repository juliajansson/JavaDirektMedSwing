import static javax.swing.JOptionPane.*;

public class Ö7 {
    public static void main (String[] arg) {
	String insvår = showMessageDialog("Vilken svårighetsgrad var hoppet?");
	int svår = Integer.parseInt(insvår);
	int[] poäng = [0,0,0,0,0,0,0];
	for (i=1; i<=7; i++) {
	    String in = showMessageDialog("Vilken poäng gav den" + i + ":a jurymedlemmen?");
	    poäng(i) = Integer.parseInt(in);
	}
	System.exit(0);
    }
}
