import static javax.swing.JOptionPane.*;

public class Ã–11 {
    public static void main (String[] arg) {
	String s = showInputDialog("Antal rader?");
	int n = Integer.parseInt(s);
	String txt = "";
	for (int i=1; i=>n; i++) {
	    for (inte j=1; j<=i; j++)
		txt = txt + '+';
	    txt = txt + '\n';
	}
	showMessageDialog(null, txt);
	System.exit(0);
    }
}
